{
  "name": "vue-compiler",
  "productName": "Vue Compiler",
  "version": "1.0.0",
  "description": "'compile vue files'",
  "main": "main.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "path": "^0.12.7",
    "vue-devtools": "^5.1.0",
    "vue-to-js": "^0.9.4"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-win": "electron-packager . --electron-version=2.0.18 --overwrite --asar=true --platform=win32 --icon=./app/images/icon.ico --ignore=^/builds --out=builds",
    "build-linux": "electron-packager . --electron-version=2.0.18 --overwrite --asar=true --platform=linux --icon=./app/images/icon.png --ignore=^/builds --out=builds",
    "build-darwin": "electron-packager . --electron-version=2.0.18 --overwrite --asar=true --platform=darwin --icon=./app/images/icon.icns --ignore=^/builds --out=builds"
  },
  "keywords": [
    "vue",
    "compile",
    "ui"
  ],
  "author": "Akbar",
  "license": "ISC"
}
